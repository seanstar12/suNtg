{
  "name": "su@NTG",
  "version": "0.4.20",
  "manifest_version": 2,
  "description": "Because life could suck less.",
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": { 
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "alarms",
    "tabs", 
    "*://*.missouristate.edu/",
    "background",
    "cookies",
    "storage" 
  ],
  "web_accessible_resources": [
    "js/jquery-1.8.3.min.js",
    "js/bootstrap.min.js",
    "js/functions.js",
    "js/reload.js",
    "js/handlebars.js",
    "js/tabber-minimized.js",
    "js/pop.js",
    "js/jquery-plugins.js",
    "reload.html",
    "portal.html",
    "js/jquery-ui.min.js",
    "js/jquery.form.js",
    "css/bootstrap.css",
    "css/clear.css",
    "css/ntgTool.css",
    "images/glyphicons-halflings-white.png",
    "images/glyphicons-halflings.png"
  ],
  "content_scripts": [
    {
      "js": ["js/onStart.js"],
      "matches": ["https://ntg.missouristate.edu/*"],
      "css":["css/clear.css","css/ntgTool.css"],
      "run_at": "document_start"
    },
    {
      "js": ["js/jquery-1.8.3.min.js","js/functions.js","js/handlebars.js","js/onEnd.js"],
      "matches": ["https://ntg.missouristate.edu/*"],
      "run_at": "document_end"
    }
  ]
}
