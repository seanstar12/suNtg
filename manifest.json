{
  "name": "su@NTG",
  "version": "0.4.23",
  "manifest_version": 2,
  "description": "It puts the lotion on the skin.",
  "options_page": "ooptions.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": { 
    "page": "background.html"
  },
  "page_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "alarms",
    "tabs", 
    "*://*.missouristate.edu/",
    "background",
    "cookies",
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "js/*",
    "css/*",
    "images/*",
    "lib/*",
    "portal.html",
    "reload.html"
  ],
  "content_scripts": [
    {
      "js": [
        "js/onStart.js"
      ],
      "matches": ["https://ntg.missouristate.edu/*","https://ntgdev.missouristate.edu/*"],
      "css":["css/clear.css","css/ntgTool.css"],
      "run_at": "document_start"
    },
    {
      "js": [
        "js/jquery-2.0.2.min.js",
        "js/ntgVars.js",
        "js/handlebars.js",
        "js/templates.js",
        "js/bootstrap.min.js",
        "js/functions.js",
        "js/onEnd.js"
      ],
      "matches": ["https://ntg.missouristate.edu/*","https://ntgdev.missouristate.edu/*"],
      "run_at": "document_end"
    }
  ]
}
